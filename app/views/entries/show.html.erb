<p id="notice"><%= notice %></p>

<div class="row container-fluid entries">
  <div class="entry container">
    <h2><%= @entry.title %></h2>
    <p><%= @entry.text %></p>
    <img src="<%= @entry.image %>"></img>
    <img src="<%= @entry.video %>"></img>
  </div>
</div>

<%= link_to 'Edit', edit_entry_path(@entry) %> |
<%= link_to 'Back', entries_path %>
